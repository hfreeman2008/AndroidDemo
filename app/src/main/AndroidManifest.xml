<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.hxm.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
                  >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity

            android:name=".swipelistview.SimpleActivity"/>
        <activity android:name=".swipelistview.DifferentMenuActivity"/>
        <activity
            android:label="@string/swip_menu_listview"
            android:name=".swipelistview.SwipeMainActivity"/>
        <activity
            android:label="@string/custer_image"
            android:name=".circularimageview.CircularImageViewActivity">
        </activity>
        <activity
            android:label="@string/auto_scroll_view_paper"
            android:name=".autoscrollviewpager.AutoScrollViewPaperActivity">
        </activity>
    </application>

</manifest>